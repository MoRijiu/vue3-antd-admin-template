{
  "name": "vue3-antd-admin-template",
  "version": "1.0.0",
  "private": false,
  "description": "A modern admin template built with Vue 3, TypeScript, Ant Design Vue, and Vite. Features include authentication, routing, state management, i18n, and more.",
  "type": "module",
  "author": {
    "name": "MoRijiu",
    "email": "1092257069@qq.com",
    "url": "https://github.com/MoRijiu/vue3-antd-admin-template"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoRijiu/vue3-antd-admin-template.git"
  },
  "bugs": {
    "url": "https://github.com/MoRijiu/vue3-antd-admin-template/issues"
  },
  "keywords": [
    "vue3",
    "typescript",
    "vite",
    "ant-design-vue",
    "admin-template",
    "vue-admin",
    "pinia"
  ],
  "engines": {
    "node": ">=20.19.1",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --mode development",
    "dev:local": "vite --mode local",
    "dev:test": "vite --mode test",
    "build": "vue-tsc -b && vite build --mode production",
    "build:dev": "vue-tsc -b && vite build --mode development",
    "build:test": "vue-tsc -b && vite build --mode test",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:test": "vite preview --mode test",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf dist node_modules/.vite",
    "commit": "git-cz",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@vueuse/core": "^14.0.0",
    "@vueuse/motion": "^3.0.3",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^1.0.2",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.13.2",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "echarts": "^6.0.0",
    "file-saver": "^2.0.5",
    "js-md5": "^0.8.3",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "qs": "^6.14.0",
    "vue": "^3.5.24",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@unocss/preset-uno": "^66.5.6",
    "@unocss/reset": "^66.5.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "mockjs": "^1.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.94.0",
    "typescript": "~5.9.3",
    "unocss": "^66.5.6",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.1",
    "vite-plugin-mock": "^3.0.2",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.3"
  }
}
