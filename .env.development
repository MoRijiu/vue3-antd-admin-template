# ==================== 开发环境变量 ====================
# 开发环境配置

# 环境标识
VITE_ENV=development

# 应用端口
VITE_PORT=3000

# 是否自动打开浏览器
VITE_OPEN=true

# API 基础路径
VITE_API_BASE_URL=/api

# API 代理目标地址（开发服务器）
VITE_API_PROXY_TARGET=https://dev-api.example.com

# 是否启用 Mock 数据
VITE_USE_MOCK=true

# 是否启用 HTTPS
VITE_HTTPS=false

# 是否启用压缩
VITE_BUILD_COMPRESS=false

# 是否删除 console
VITE_DROP_CONSOLE=false

# 是否生成 sourcemap
VITE_SOURCEMAP=true

# 上传文件大小限制（MB）
VITE_UPLOAD_SIZE=10

# Token 存储 key
VITE_TOKEN_KEY=ACCESS_TOKEN

# Token 过期时间（小时）
VITE_TOKEN_EXPIRE=24

# 是否启用路由权限
VITE_USE_PERMISSION=true

# 是否启用页面缓存
VITE_USE_CACHE=true

