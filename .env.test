# ==================== 测试环境变量 ====================
# 测试环境配置

# 环境标识
VITE_ENV=test

# 应用端口
VITE_PORT=3001

# 是否自动打开浏览器
VITE_OPEN=false

# API 基础路径
VITE_API_BASE_URL=/api

# API 代理目标地址（测试服务器）
VITE_API_PROXY_TARGET=https://test-api.example.com

# 是否启用 Mock 数据
VITE_USE_MOCK=false

# 是否启用 HTTPS
VITE_HTTPS=false

# 是否启用压缩
VITE_BUILD_COMPRESS=true

# 是否删除 console
VITE_DROP_CONSOLE=true

# 是否生成 sourcemap
VITE_SOURCEMAP=true

# 上传文件大小限制（MB）
VITE_UPLOAD_SIZE=20

# Token 存储 key
VITE_TOKEN_KEY=ACCESS_TOKEN

# Token 过期时间（小时）
VITE_TOKEN_EXPIRE=12

# 是否启用路由权限
VITE_USE_PERMISSION=true

# 是否启用页面缓存
VITE_USE_CACHE=true

